#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
  int n,i;
  char x[100];
  clrscr();
  printf("\nEnter the character to check whether the word is ReeWord or not:");
  scanf("%s",x);
  n=strlen(x);
  for(i=0;i<n;i++)
  {
    if(n>=6&&n<=20)
    {
      if(x[i]=='r')
      {
	if(x[i]=='#'||x[i]=='$'||x[i]=='!'||x[i]=='@')
	{
	  if(x[i]!=' ')
	  {
	    printf("\nReeWord");
	  }
	}
      }
    }
  }
  printf("\nReeword");
  if(n<6||n>20)
	printf("Nay!");
 getch();
}
